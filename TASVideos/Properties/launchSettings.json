{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "https://localhost:44385/",
      "sslPort": 44385
    }
  },
  "profiles": {
    "Staging": {
      "commandName": "Project",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Staging"
      }
    },
    "Dev Postgres (No recreate)": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "https://localhost:44385/",
      "environmentVariables": {
        "ASPNETCORE_preventHostingStartup": "True",
        "ASPNETCORE_ENVIRONMENT": "Development",
        "StartupStrategy": "Minimal"
      }
    },
    "Dev Postgres (Sample Data)": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "https://localhost:44385/",
      "environmentVariables": {
        "ASPNETCORE_preventHostingStartup": "True",
        "ASPNETCORE_ENVIRONMENT": "Development",
        "StartupStrategy": "Sample"
      }
    },
    "Dev Postgres (Migrate)": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "https://localhost:44385/",
      "environmentVariables": {
        "ASPNETCORE_preventHostingStartup": "True",
        "ASPNETCORE_ENVIRONMENT": "Development",
        "StartupStrategy": "Migrate"
      }
    }
  }
}